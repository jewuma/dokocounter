layersums\n    };\n  },\n  async created() {\n    this.refreshState();\n  },\n  emits: [\"back\"],\n  methods: {\n    async refreshState() {\n      this.games = await back.fetchActiveList();\n      Object.values(this.games).forEach(game => {\n        game.players.forEach(player => {\n          if (this.sums[player.playerId] === undefined) this.sums[player.playerId] = 0;\n          this.sums[player.playerId] += player.points * 1;\n        })\n      })\n      this.players = await back.fetchPlayers();\n      this.players = this.players.filter(player => player.active == true).sort((playerA, playerB) => this.sums[playerA.id] - this.sums[playerB.id]);\n    },\n  }\n})\n</script>\n<style scoped>\n.games {\n  width: 100%\n}\n\n.gameHead {\n  text-align: right;\n  background-color: yellow;\n  font-weight: bold;\n}\n\n.gameTable {\n  text-align: right;\n  background-color: azure;\n}\n</style>"]}]}