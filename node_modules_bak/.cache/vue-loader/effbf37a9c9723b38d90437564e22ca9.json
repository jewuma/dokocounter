    <DokoPlayers v-if=\"selected === 'playerList'\" @back=\"changeMenuItem('menu')\" @new-list=\"startNewList\" />\n    <Game v-if=\"selected === 'game'\" @back=\"changeMenuItem('menu')\" />\n    <GameList v-if=\"selected === 'gameList'\" @back=\"changeMenuItem('menu')\" />\n    <Statistics v-if=\"selected === 'statistics'\" @back=\"changeMenuItem('menu')\" />\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from \"vue\";\nimport DokoPlayers from \"./components/DokoPlayers.vue\";\nimport Menu from \"./components/Menu.vue\";\nimport Statistics from \"./components/Statistics.vue\";\nimport Game from \"./components/Game.vue\";\nimport GameList from \"./components/GameList.vue\";\nimport { backend, player } from \"./backend\";\nconst back = new backend();\nexport default defineComponent({\n  name: \"App\",\n  components: {\n    Menu,\n    DokoPlayers,\n    Statistics,\n    GameList,\n    Game\n  },\n  players: [],\n  host: \"\",\n  data() {\n    return {\n      players: [] as player[],\n      showAddPlayer: false as boolean,\n      selected: \"menu\" as string,\n    };\n  },\n  methods: {\n    changeMenuItem(item: string) {\n      this.selected = item;\n    },\n    async startNewList() {\n      await back.startNewList();\n      this.changeMenuItem('game');\n    }\n  },\n  async created() {\n    this.selected = \"menu\";\n  },\n});\n</script>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap\");\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  font-family: \"Poppins\", sans-serif;\n}\n\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n\n.btn:focus {\n  outline: none;\n}\n\n.btn:active {\n  transform: scale(0.98);\n}\n\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"]}]}