AAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;EACrD,CAAC,CAAC,CAAC,CAAC,CAAC","file":"Z:/doko3/src/components/Game.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <Button\n      :key=\"key\"\n      v-for=\"(player, key) in scores\"\n      :text=\"player.name\"\n      :textr=\"player.score.toString()\"\n      :color=\"player.party === kontraRe.Kontra ? 'green' : 'red'\"\n      @click=\"toggleParty(key)\"\n    />\n    <input type=\"range\" id=\"result\" min=\"-5\" max=\"+6\" />\n  </div>\n</template>\n<script lang=\"ts\">\nimport { defineComponent } from \"vue\";\nimport { player, backend, kontraRe } from \"../backend\";\nimport Button from \"./Button.vue\";\nconst back = new backend();\n\ninterface playerState {\n  [playerId: number]: {\n    name: string;\n    score: number;\n    party: kontraRe;\n  };\n}\nexport default defineComponent({\n  name: \"Game\",\n  data() {\n    return {\n      kontraRe,\n      gameState: [] as string[][],\n      players: [] as player[],\n      buttons: [] as string[],\n      scores: {} as playerState,\n    };\n  },\n  async created() {\n    this.gameState = await back.fetchActiveList();\n    this.players = await back.fetchPlayers();\n    this.buttons[0] = \"TestText\";\n    this.scores = {};\n    Object.values(this.gameState).forEach((singleGame) => {\n      //console.log(singleGame);\n      for (let playerId in singleGame) {\n        let singleScore = singleGame[playerId];\n        console.log(singleGame[playerId]);\n        if (this.scores[playerId] === undefined) {\n          this.scores[playerId] = {\n            name: this.players[playerId].name,\n            score: 0,\n            party: kontraRe.Kontra,\n          };\n        }\n        this.scores[playerId].score += parseInt(singleScore);\n      }\n    });\n    console.log(this.scores);\n  },\n  components: {\n    Button,\n  },\n  methods: {\n    toggleParty(playerId: number) {\n      if (this.scores[playerId].party === kontraRe.Kontra) {\n        this.scores[playerId].party = kontraRe.Re;\n      } else {\n        this.scores[playerId].party = kontraRe.Kontra;\n      }\n    },\n  },\n});\n</script>\n"]}]}